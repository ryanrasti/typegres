{
  "name": "typegres-examples",
  "version": "1.0.0",
  "description": "Examples for Typegres",
  "private": true,
  "type": "module",
  "scripts": {
    "examples": "npm run example:basic && npm run example:advanced",
    "example:basic": "tsx basic-example.ts",
    "example:advanced": "tsx advanced-example.ts",
    "test": "vitest run",
    "setup": "cd .. && npm run build && npm pack --pack-destination examples && cd examples && npm install"
  },
  "dependencies": {
    "tsafe": "^1.8.5",
    "typegres": "file:./typegres-0.0.1.tgz"
  },
  "devDependencies": {
    "@types/node": "^24.0.12",
    "tsx": "^4.7.1",
    "vitest": "^3.2.4"
  }
}
