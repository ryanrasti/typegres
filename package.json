{
  "name": "typegres",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:cli",
    "build:lib": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "build:cli": "tsup src/cli/main.ts --format cjs --no-dts --out-dir dist --entry.tg src/cli/main.ts --platform node --minify --target node20",
    "build:bundle": "rollup -c",
    "build:tsgo": "tsgo",
    "preview": "vite preview",
    "test": "vitest run --root src/",
    "typecheck": "tsgo --noEmit",
    "lint": "eslint",
    "prepare": "true",
    "format": "prettier --write \"**/*.{ts,js,json}\" --log-level warn",
    "format:check": "prettier --check \"**/*.{ts,js,json}\"",
    "check": "npm test && npm run typecheck && npm run lint && npm run format:check",
    "codegen": "bash ./src/gen/gen.sh && kysely --no-outdated-check migrate:latest -e test && npm run codegen:tables",
    "codegen:tables": "./bin/tg introspect -d postgres://postgres:postgres@localhost:1234/test > ./src/gen/tables.ts",
    "clean": "(rm ./src/gen/functions.ts; rm -r ./src/gen/types; rm ./src/gen/tables.json; rm ./src/gen/tables.ts) || true",
    "prepublishOnly": "npm version $(npm pkg get version | tr -d '\"')-$(git rev-parse --short HEAD) --no-git-tag-version"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.77",
    "@effect/cli": "^0.69.0",
    "@effect/platform": "^0.90.3",
    "@effect/platform-node": "^0.96.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/pg": "^8.11.6",
    "@typescript/native-preview": "^7.0.0-dev.20250626.1",
    "@vitest/ui": "^2.1.9",
    "cross-inspect": "^1.0.1",
    "effect": "^3.17.7",
    "eslint": "^8.42.0",
    "happy-dom": "^20.0.8",
    "kysely-ctl": "^0.9.0",
    "npm": "^10.9.0",
    "pg": "^8.12.0",
    "postgres-range": "^1.1.4",
    "prettier": "3.6.2",
    "rollup": "^4.44.1",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "ts-xor": "^1.3.0",
    "tsafe": "^1.8.5",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2",
    "vite": "^5.3.1",
    "vitest": "^2.1.9",
    "zx": "^8.5.3"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.0",
    "camelcase": "^8.0.0",
    "kysely": "^0.27.4",
    "kysely-pglite-dialect": "^1.1.1",
    "postgres-array": "^3.0.2",
    "postgres-date": "^2.1.0",
    "tiny-invariant": "^1.3.3"
  },
  "description": "PostgreSQL, expressed in TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "tg": "./dist/tg.js"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/ndex.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "author": "Ryan Rasti",
  "license": "MIT"
}
